{
  "version": 2,
  "routes": [
    { "src": "/", "dest": "/index.html" },
    { "src": "/admin", "dest": "/admin.html" },
    { "src": "/offers.json", "dest": "/offers.json" },
    { "src": "/(.*\\.json)", "dest": "/$1" },
    { "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg))", "dest": "/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
